# Solving linear Equations using Sympy

# Form of equation [A][x] = [b]

import sympy
sympy.init_printing()

A = sympy.Matrix([[2, 3], [5, 4]])
b = sympy.Matrix([4, 3])

x = A.solve(b)   




# Solving linear Equations numerically

# Form of equation [A][x] = [b]

import numpy as np
from scipy import linalg as la


A =np.array([[2,3],[5,4]])
b = np.array([4, 3])
la.solve(A, b)
